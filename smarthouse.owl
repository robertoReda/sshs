    <!-- http://example.com/smarthouse/BuildingElements -->

    <owl:Class rdf:about="http://example.com/smarthouse/BuildingElements"/>
    


    <!-- http://example.com/smarthouse/Clear -->

    <owl:Class rdf:about="http://example.com/smarthouse/Clear">
        <rdfs:subClassOf rdf:resource="http://example.com/smarthouse/WeatherCondition"/>
    </owl:Class>
    


    <!-- http://example.com/smarthouse/ClosedWindow -->

    <owl:Class rdf:about="http://example.com/smarthouse/ClosedWindow">
        <rdfs:subClassOf rdf:resource="http://example.com/smarthouse/Window"/>
        <owl:disjointWith rdf:resource="http://example.com/smarthouse/OpenWindow"/>
    </owl:Class>
    


    <!-- http://example.com/smarthouse/DesiredStates -->

    <owl:Class rdf:about="http://example.com/smarthouse/DesiredStates"/>
    


