                                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                                <rdf:first>
                                                                    <rdf:Description>
                                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                                                        <swrl:classPredicate rdf:resource="http://example.com/smarthouse/Tenant"/>
                                                                        <swrl:argument1 rdf:resource="http://example.com/smarthouse/tenant"/>
                                                                    </rdf:Description>
