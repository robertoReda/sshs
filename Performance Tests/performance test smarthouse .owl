    <rdf:Description rdf:about="http://example.com/smarthouse/d">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://example.com/smarthouse/r">
