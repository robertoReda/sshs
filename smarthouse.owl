                                                                        <rdf:first>
                                                                            <rdf:Description>
                                                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                                                <swrl:propertyPredicate rdf:resource="http://example.com/smarthouse/isLocatedIn"/>
                                                                                <swrl:argument1 rdf:resource="http://example.com/smarthouse/tenant"/>
                                                                                <swrl:argument2 rdf:resource="http://example.com/smarthouse/room"/>
                                                                            </rdf:Description>
                                                                        </rdf:first>
                                                                        <rdf:rest>
                                                                            <rdf:Description>
                                                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                                                <rdf:first>
                                                                                    <rdf:Description>
                                                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                                                                        <swrl:classPredicate rdf:resource="https://saref.etsi.org/core/LightSwitch"/>
                                                                                        <swrl:argument1 rdf:resource="http://example.com/smarthouse/light"/>
                                                                                    </rdf:Description>
                                                                                </rdf:first>
                                                                                <rdf:rest>
