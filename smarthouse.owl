    <!-- http://example.com/smarthouse/WeatherConditionRain -->

    <owl:NamedIndividual rdf:about="http://example.com/smarthouse/WeatherConditionRain">
        <rdf:type rdf:resource="http://example.com/smarthouse/Rain"/>
    </owl:NamedIndividual>
    


